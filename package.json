{
  "name": "oms-platform-app",
  "version": "1.0.0",
  "private": true,
  "description": "oms-platform-app",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:master": "pnpm run build --mode master",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",

    "dev:weapp": "npm run build:weapp -- --watch",
    "devtools:weapp": "npm run build:weapp -- --watch --devtools",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "preview:h5": "serve -s dist",
    "type:check": "tsc --noEmit",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "postinstall": "simple-git-hooks && weapp-tw patch",
    "eslint:fix": "eslint --ext .vue --ext .js --ext .jsx --ext .ts --ext .tsx --fix src/",
    "eslint": "eslint --ext .vue --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "prepare": "husky install",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "cz": "cz"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@babel/runtime": "^7.23.4",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "^4.2.2",
    "@nutui/touch-emulator": "^1.0.0",
    "@tarojs/components": "3.6.19",
    "@tarojs/helper": "3.6.19",
    "@tarojs/plugin-framework-vue3": "3.6.19",
    "@tarojs/plugin-html": "3.6.19",
    "@tarojs/plugin-platform-alipay": "3.6.19",
    "@tarojs/plugin-platform-h5": "3.6.19",
    "@tarojs/plugin-platform-jd": "3.6.19",
    "@tarojs/plugin-platform-qq": "3.6.19",
    "@tarojs/plugin-platform-swan": "3.6.19",
    "@tarojs/plugin-platform-tt": "3.6.19",
    "@tarojs/plugin-platform-weapp": "3.6.19",
    "@tarojs/runtime": "3.6.19",
    "@tarojs/shared": "3.6.19",
    "@tarojs/taro": "3.6.19",
    "@vue3-oop/taro-hooks": "^1.0.1",
    "axios": "^1.5.0",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.1",
    "taro-plugin-pinia": "^1.0.0",
    "ts-essentials": "^9.4.0",
    "tslib": "^2.6.2",
    "vue": "^3.3.8",
    "vue3-oop": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nutui/auto-import-resolver": "^1.0.0",
    "@tarojs/cli": "^3.6.19",
    "@tarojs/mini-runner": "^3.6.19",
    "@tarojs/plugin-http": "^3.6.19",
    "@tarojs/plugin-mock": "^0.0.9",
    "@tarojs/taro-loader": "3.6.19",
    "@tarojs/webpack5-runner": "3.6.19",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.3",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.3.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue3-oop/taro-plugin": "^1.0.5",
    "babel-plugin-import": "^1.13.8",
    "babel-preset-taro": "3.6.19",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "6.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.49.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-taro": "3.6.16",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.30",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "serve": "^14.2.1",
    "simple-git-hooks": "^2.9.0",
    "style-loader": "3.3.3",
    "swagger-schema-official": "2.0.0-bab6bed",
    "tailwindcss": "~3.3.3",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "ts-gear": "^4.11.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vue-loader": "^17.2.2",
    "vue-tsc": "^1.8.24",
    "weapp-tailwindcss": "^2.9.2",
    "webpack": "^5.89.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
